{
  "name": "nsi-frontend-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "serve:api": "json-server --watch db.json --port 3000",
    "dev:all": "concurrently -n API,WEB -c magenta,cyan \"npm:serve:api\" \"npm:dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.86.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/tabler": "^0.13.0",
    "axios": "^1.11.0",
    "naive-ui": "^2.43.1",
    "pinia": "^3.0.3",
    "vfonts": "^0.0.3",
    "vue": "^3.5.18",
    "vue-i18n": "9.14.5",
    "vue-router": "^4.5.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^16.2.3",
    "msw": "^2.11.5",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass": "^1.93.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "lint-staged": {
    "*.{ts,vue,js,json,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
